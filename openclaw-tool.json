{
    "name": "clanker_deploy_token",
    "description": "Deploy an ERC-20 token on Base blockchain with automated liquidity via Clanker SDK v4. Returns JSON with deployment result.",
    "parameters": {
        "type": "object",
        "properties": {
            "name": {
                "type": "string",
                "description": "Token name (e.g., 'ClawBot AI')"
            },
            "symbol": {
                "type": "string",
                "description": "Token ticker symbol (e.g., 'CLAW')"
            },
            "image": {
                "type": "string",
                "description": "Token logo - IPFS CID (bafkrei...) or HTTPS URL"
            },
            "description": {
                "type": "string",
                "description": "Token/project description"
            },
            "admin": {
                "type": "string",
                "description": "Wallet address that will own the token/LP"
            },
            "fees": {
                "type": "object",
                "description": "Fee configuration (100 bps = 1%)",
                "properties": {
                    "type": {
                        "type": "string",
                        "enum": [
                            "static",
                            "dynamic"
                        ],
                        "default": "static"
                    },
                    "mode": {
                        "type": "string",
                        "enum": [
                            "static",
                            "dynamic"
                        ],
                        "description": "Alias for type"
                    },
                    "clankerFee": {
                        "type": "integer",
                        "description": "Token side fee in bps",
                        "default": 300
                    },
                    "pairedFee": {
                        "type": "integer",
                        "description": "WETH side fee in bps",
                        "default": 300
                    },
                    "baseFee": {
                        "type": "integer",
                        "description": "Dynamic base fee in bps (default 100 = 1%)"
                    },
                    "maxFee": {
                        "type": "integer",
                        "description": "Dynamic max fee in bps (default 1000 = 10%)"
                    },
                    "adjustmentPeriod": {
                        "type": "integer",
                        "description": "Dynamic reference tick filter period in seconds (default 3600)"
                    },
                    "resetPeriod": {
                        "type": "integer",
                        "description": "Dynamic reset period in seconds (default 86400)"
                    },
                    "resetTickFilter": {
                        "type": "integer",
                        "description": "Dynamic reset tick filter (default 100)"
                    },
                    "feeControlNumerator": {
                        "type": "integer",
                        "description": "Dynamic fee control numerator (default 100000)"
                    },
                    "decayFilterBps": {
                        "type": "integer",
                        "description": "Dynamic decay filter in bps (default 9500)"
                    }
                }
            },
            "context": {
                "type": "object",
                "description": "Source context link for indexing. Platform can be auto-detected from URL.",
                "properties": {
                    "platform": {
                        "type": "string",
                        "enum": [
                            "twitter",
                            "farcaster",
                            "clanker",
                            "website",
                            "telegram",
                            "discord",
                            "github",
                            "medium",
                            "reddit",
                            "instagram",
                            "youtube",
                            "tiktok",
                            "linkedin"
                        ],
                        "default": "twitter"
                    },
                    "messageId": {
                        "type": "string",
                        "description": "Optional explicit context ID. Usually auto-fetched from context URL."
                    },
                    "url": {
                        "type": "string",
                        "description": "Source URL for context. Platform and messageId can be auto-detected."
                    }
                }
            },
            "socials": {
                "type": "object",
                "properties": {
                    "x": {
                        "type": "string"
                    },
                    "telegram": {
                        "type": "string"
                    },
                    "farcaster": {
                        "type": "string"
                    },
                    "website": {
                        "type": "string"
                    }
                }
            },
            "sniperFees": {
                "type": "object",
                "description": "Anti-bot protection (Unibps: 1,000,000 = 100%)",
                "properties": {
                    "startingFee": {
                        "type": "integer",
                        "default": 666777
                    },
                    "endingFee": {
                        "type": "integer",
                        "default": 41673
                    },
                    "secondsToDecay": {
                        "type": "integer",
                        "default": 15
                    }
                }
            },
            "devBuy": {
                "type": "number",
                "description": "ETH amount to buy at launch (optional)"
            },
            "vanity": {
                "type": "boolean",
                "description": "Request Clanker vanity address suffix",
                "default": true
            },
            "dryRun": {
                "type": "boolean",
                "description": "Validate config without deploying",
                "default": false
            },
            "strictMode": {
                "type": "boolean",
                "description": "Enforce Clankerworld Blue Badge requirements (5% max fee, farcaster only)",
                "default": false
            },
            "requireContext": {
                "type": "boolean",
                "description": "Fail deployment if context messageId is missing (recommended)",
                "default": true
            },
            "smartValidation": {
                "type": "boolean",
                "description": "Auto-heal missing/invalid fields instead of failing validation",
                "default": true
            },
            "privateKey": {
                "type": "string",
                "description": "Deployer private key (optional - uses .env if not provided)"
            },
            "rpcUrl": {
                "type": "string",
                "description": "Primary Base RPC endpoint"
            },
            "rpcFallbackUrls": {
                "type": "string",
                "description": "Comma-separated RPC fallback endpoints for failover"
            },
            "adminSpoof": {
                "type": "string",
                "description": "Spoof admin target address (legacy env mode)"
            },
            "rewardCreator": {
                "type": "string",
                "description": "Primary reward recipient (usually your wallet)"
            },
            "rewardInterface": {
                "type": "string",
                "description": "Interface/spoof reward recipient (usually target address)"
            },
            "rewardCreatorAdmin": {
                "type": "string",
                "description": "Admin address for rewardCreator recipient"
            },
            "rewardInterfaceAdmin": {
                "type": "string",
                "description": "Admin address for rewardInterface recipient"
            },
            "spoof": {
                "type": "object",
                "description": "Shortcut spoof block for OpenClaw env mapping",
                "properties": {
                    "enabled": {
                        "type": "boolean"
                    },
                    "ourWallet": {
                        "type": "string"
                    },
                    "targetAddress": {
                        "type": "string"
                    }
                }
            }
        }
    },
    "returns": {
        "type": "object",
        "properties": {
            "success": {
                "type": "boolean"
            },
            "address": {
                "type": "string",
                "description": "Deployed token address"
            },
            "txHash": {
                "type": "string"
            },
            "scanUrl": {
                "type": "string"
            },
            "error": {
                "type": "string"
            }
        }
    }
}
