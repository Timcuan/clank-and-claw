{
  "name": "clank-and-claw",
  "version": "2.7.0",
  "description": "Clanker Token Deployment - Telegram Bot & AI Agent",
  "main": "deploy.js",
  "scripts": {
    "start": "node telegram-bot.js",
    "deploy": "node deploy.js",
    "spoof": "node deploy.js --spoof",
    "setup": "node setup.js",
    "vps:wizard": "bash vps-manager.sh wizard",
    "vps:update": "bash vps-manager.sh update",
    "vps:telegram": "bash vps-manager.sh telegram-setup",
    "vps:kubo": "bash vps-manager.sh kubo-install",
    "vps:ipfs": "bash vps-manager.sh ipfs-setup",
    "vps:status": "bash vps-manager.sh status",
    "test": "DRY_RUN=true node deploy.js",
    "test:hardening": "node --test test/*.test.js"
  },
  "keywords": [
    "clanker",
    "base",
    "token",
    "deployment",
    "telegram"
  ],
  "author": "Timcuan",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "clanker-sdk": "^4.2.10",
    "dotenv": "^17.2.4",
    "viem": "^2.45.1"
  }
}
